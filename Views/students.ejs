<head>
    <h1>Students</h1>
</head>
<body>
        <a href="/addStudent">Add Student</a><br>
        <a href="/">Home</a>
    <table border="1">
        <thead>
            <tr>
                <th>Student ID</th>
                <th>Name</th>
                <th>Age</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <% students.forEach(student =>{ %>
                <tr>
                    <td><%= student.sid %></td>
                    <td><%= student.name %></td>
                    <td><%= student.age %></td>
                    <td><a href="/students/update/<%=student.sid%>">Update</a></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>